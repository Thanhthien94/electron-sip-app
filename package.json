{
  "name": "sip-app",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Thanhthien94",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "NODE_ENV=development electron-vite dev",
    "dev:win": "SET NODE_ENV=development& electron-vite dev",
    "build": "rm -rf dist && npm run typecheck && NODE_ENV=production electron-vite build",
    "build:win-prepare": "rmdir /s /q dist && npm run typecheck && SET NODE_ENV=production& electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build:win-prepare && electron-builder --win --ia32",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:all": "npm run build && electron-builder -mwl",
    "env:create": "node ./scripts/create-env.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thanhthien94/electron-sip-app"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-updater": "^6.1.7",
    "ffmpeg-static": "^5.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.2.4",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "jssip": "^3.10.1",
    "lucide-react": "^0.468.0",
    "moment-timezone": "^0.5.46",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.7",
    "react-toastify": "^11.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
